#include <string>
using namespace std;
//ANSI codes 
#define RESET   "\033[0m"
#define RED     "\033[1;31m"
#define DRED    "\033[0;31m"
#define ORANGE  "\033[1;33m"
#define DORANGE "\033[0;33m"
#define BLUE    "\033[1;34m"
#define DBLUE   "\033[0;34m"
#define GREEN   "\033[1;32m"
#define DGREEN  "\033[0;32m"
